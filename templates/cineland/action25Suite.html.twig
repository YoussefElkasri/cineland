{% block contenu %}
<div class="col-md-12">
    <div class="content-panel">
    <table class="table table-striped table-advance table-hover" id="dynamictable">
        <thead>
        <tr>
            <th> #ID</th>
            <th> TITRE</th>
            <th> DUREE</th>
            <th> DATE SORTIE</th>
            <th> NOTE</th>
            <th> AGE MINIMAL</th>

        </tr>
        </thead>
        <tbody>

        {% for film in films%}
            <tr>

                <td >{{film.id}}</td>
                <td>{{film.titre}}</td>
                <td>{{film.duree}}</td>
                <td>{{film.DateSortie|date('d-m-Y')}}</td>
                <td>{{film.note}}</td>
                <td>{{film.ageMinimal}}</td>

            </tr>
        {% endfor %}
        </tbody>
    </table>
    </div>
    <a href="{{ path('action25') }}" ><button style="position: relative;top: 10px;" class="btn btn-primary">Retour au recherche</button></a>
</div>
{% endblock %}
